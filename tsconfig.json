{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./bin",
    "strict": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noFallthroughCasesInSwitch": true,
    "declaration": true
  },
  "files": [
    "src/Entrypoint.ts",
    "src/Momiji.ts",
    "src/Command.ts",
    "src/commands/Pong.ts",
    "src/commands/Roll.ts"
  ]
}
